{
	"name": "console-express-routes",
	"version": "1.0.2",
	"author": "yutak23 <yuta.katayama.23+github@gmail.com> (https://github.com/yutak23)",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:yutak23/console-express-routes.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"license": "MIT",
	"type": "module",
	"types": "./index.d.ts",
	"main": "./cjs/index.js",
	"module": "./index.js",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js",
			"require": "./cjs/index.js"
		}
	},
	"scripts": {
		"build": "run-s clean build:*",
		"build:cjs": "babel index.js --out-dir cjs && echo '{\"type\":\"commonjs\"}' > cjs/package.json",
		"clean": "rm -rf ./cjs",
		"test": "vitest run --coverage",
		"lint": "eslint . --ext .cjs,.mjs,.js",
		"lint:fix": "eslint . --ext .cjs,.mjs,.js --fix",
		"format": "prettier -c .",
		"format:fix": "prettier --write .",
		"ncu": "ncu"
	},
	"dependencies": {
		"chalk": "^5.3.0",
		"cli-table3": "^0.6.3",
		"express": "^4.18.2",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.0",
		"@babel/core": "^7.23.3",
		"@babel/preset-env": "^7.23.3",
		"@types/express": "^4.17.21",
		"@vitest/coverage-v8": "^0.34.6",
		"eslint": "^8.54.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.29.0",
		"npm-check-updates": "^16.14.6",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.1.0",
		"vitest": "^0.34.6"
	},
	"files": [
		"cjs",
		"index.d.ts",
		"index.js"
	],
	"keywords": [
		"node",
		"nodejs",
		"javascript",
		"js",
		"library",
		"lib",
		"module",
		"typescript",
		"ts",
		"esm",
		"express",
		"routes"
	]
}
